[
    {
        "name": "dagit",
        "image": "${dagit-image}",
        "cpu": 100,
        "memory": 128,
        "essential": true,
        "portMappings": [
            {
                "protocol": "tcp",
                "containerPort": 3000
            }
        ],
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
                "awslogs-group": "${cloudwatch-log-group}",
                "awslogs-region": "eu-west-1",
                "awslogs-stream-prefix": "logs"
            }
        },
        "environment": [
            {
                "name": "DAGSTER_POSTGRES_HOSTNAME",
                "value": "${rds-hostname}"
            },
            {
                "name": "DAGSTER_POSTGRES_USER",
                "value": "user"
            },
            {
                "name": "DAGSTER_POSTGRES_PASSWORD",
                "value": "${rds-password}"
            },
            {
                "name": "DAGSTER_POSTGRES_DB",
                "value": "dagsterdb"
            },
            {
                "name": "HOSTNAME_etl",
                "value": "etl"
            }
        ],
        "entryPoint": [
            "dagit",
            "-h",
            "0.0.0.0",
            "-p",
            "3000",
            "-w",
            "workspace.yaml"
        ],
        "mountPoints": [
            {
                "sourceVolume": "docker_sock",
                "containerPath": "/var/run/docker.sock",
                "readOnly": true
            }
        ]
    },
    {
        "name": "daemon",
        "image": "${daemon-image}",
        "cpu": 100,
        "memory": 128,
        "essential": true,
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
                "awslogs-group": "${cloudwatch-log-group}",
                "awslogs-region": "eu-west-1",
                "awslogs-stream-prefix": "logs"
            }
        },
        "environment": [
            {
                "name": "DAGSTER_POSTGRES_HOSTNAME",
                "value": "${rds-hostname}"
            },
            {
                "name": "DAGSTER_POSTGRES_USER",
                "value": "user"
            },
            {
                "name": "DAGSTER_POSTGRES_PASSWORD",
                "value": "${rds-password}"
            },
            {
                "name": "DAGSTER_POSTGRES_DB",
                "value": "dagsterdb"
            },
            {
                "name": "HOSTNAME_etl",
                "value": "etl"
            }
        ],
        "entryPoint": [
            "dagster-daemon",
            "run"
        ],
        "mountPoints": [
            {
                "sourceVolume": "docker_sock",
                "containerPath": "/var/run/docker.sock",
                "readOnly": true
            }
        ]
    },
    {
        "name": "etl",
        "image": "${etl-image}",
        "cpu": 100,
        "memory": 128,
        "essential": false,
        "portMappings": [
            {
                "protocol": "tcp",
                "containerPort": 4000
            }
        ],
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
                "awslogs-group": "${cloudwatch-log-group}",
                "awslogs-region": "eu-west-1",
                "awslogs-stream-prefix": "logs"
            }
        },
        "environment": [
            {
                "name": "DAGSTER_POSTGRES_HOSTNAME",
                "value": "${rds-hostname}"
            },
            {
                "name": "DAGSTER_POSTGRES_USER",
                "value": "user"
            },
            {
                "name": "DAGSTER_POSTGRES_PASSWORD",
                "value": "${rds-password}"
            },
            {
                "name": "DAGSTER_POSTGRES_DB",
                "value": "dagsterdb"
            },
            {
                "name": "DAGSTER_CURRENT_IMAGE",
                "value": "${ml-image}"
            }
        ],
        "mountPoints": [
            {
                "sourceVolume": "docker_sock",
                "containerPath": "/var/run/docker.sock",
                "readOnly": true
            }
        ]
    }
]